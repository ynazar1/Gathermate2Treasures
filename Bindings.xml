<Bindings>
	<Binding name="TOGGLE_GATHERMATE2TREASURES_MINIMAPICONS" header="GatherMate2Treasures" category="ADDONS">
		GatherMate2Treasures.db.profile["showMinimap"] = not GatherMate2Treasures.db.profile["showMinimap"]
		GatherMate2Treasures:GetModule("Config"):UpdateConfig()
		--LibStub("AceConfigRegistry-3.0"):NotifyChange("GatherMate2Treasures")
	</Binding>
	<Binding name="TOGGLE_GATHERMATE2TREASURES_MAINMAPICONS" category="ADDONS">
		GatherMate2Treasures.db.profile["showWorldMap"] = not GatherMate2Treasures.db.profile["showWorldMap"]
		GatherMate2Treasures:GetModule("Config"):UpdateConfig()
		--LibStub("AceConfigRegistry-3.0"):NotifyChange("GatherMate2Treasures")
	</Binding>
</Bindings>
